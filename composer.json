{
    "name": "takeru-yamamoto/laravel-custom-request",
    "description": "Easy to use Form Request for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "LaravelCustomRequest\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCustomRequest\\ServiceProvider"
            ],
            "aliases": {
                "InputParameter": "LaravelCustomRequest\\InputParameter\\Facade\\InputParameter"
            }
        }
    },
    "authors": [
        {
            "name": "Takeru Yamamoto",
            "email": "yamamo9150@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "laravel/framework": "^11.0",
        "takeru-yamamoto/simple-logger": "^1.0"
    },
    "minimum-stability": "dev"
}